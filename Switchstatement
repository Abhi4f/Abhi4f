
public class Switchstatement {
    public static void main(String[] args) {
        int a=20;
        switch(a){
            case 10:
                System.out.println("a is 10");
                break;
                case 20:
                    System.out.println("a is 20");
                    break;
                    case 30:
                        System.out.println("a is 30");
                        break;
            default:
                System.out.println("a is not 10,20 or 30");

                String b="This is First switch statement";
                switch(b){
                    case "  This is First switch statement" :
                        System.out.println("This is First switch statement");
                        break;
                        case"This is Second switch statement" :
                            System.out.println("This is Second switch statement");
                }
        }
    }
}
